def semaphore = createSemaphore 1
def out2=0
parallel(
        semaphore1: {
            acquireSemaphore semaphore
            echo "out1 1"
            sleep 3
            releaseSemaphore semaphore


        },
        semaphore2: {
            sleep 1
            acquireSemaphore semaphore
            echo "out2 2"
            releaseSemaphore semaphore

        }
)

